    <link rel="shortcut icon" href={static_path(@conn, "/images/favs/favicon.ico")} />
    <meta name="msapplication-config" content={static_path(@conn, "/images/favs/browserconfig.xml")} />

    <script defer type="text/javascript" src={static_path(@conn, "/assets/app.js")}></script>

  </head>
  <body>
    <%= @inner_content %>
  </body>
</html>
